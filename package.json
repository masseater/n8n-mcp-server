{
  "name": "@masseater/n8n-mcp-server",
  "version": "1.0.0",
  "description": "Model Context Protocol server for n8n workflow automation platform",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "n8n-mcp-server": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=22.10.0"
  },
  "packageManager": "pnpm@10.19.0",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsx --watch src/index.ts",
    "dev:http": "tsx --watch src/index.ts --transport http",
    "start:stdio": "node dist/index.js --transport stdio",
    "start:http": "node dist/index.js --transport http",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "knip": "knip",
    "knip:fix": "knip --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "generate:client": "openapi-ts && bash scripts/add-ts-nocheck.sh"
  },
  "keywords": [
    "n8n",
    "mcp",
    "model-context-protocol",
    "workflow",
    "automation",
    "ai",
    "llm",
    "claude",
    "workflow-automation",
    "n8n-integration",
    "mcp-server"
  ],
  "author": "masseater",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/masseater/n8n-mcp-server.git"
  },
  "homepage": "https://github.com/masseater/n8n-mcp-server#readme",
  "bugs": {
    "url": "https://github.com/masseater/n8n-mcp-server/issues"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.1",
    "commander": "^14.0.1",
    "express": "5.1.0",
    "remeda": "2.32.0",
    "tslib": "2.8.1",
    "winston": "^3.18.3",
    "zod": "^3.23.8",
    "zod-to-json-schema": "3.24.6"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "0.86.8",
    "@types/express": "5.0.3",
    "@types/node": "^24.9.1",
    "@vitest/coverage-v8": "^4.0.2",
    "eslint": "9.38.0",
    "eslint-config-flat-gitignore": "2.1.0",
    "knip": "5.66.3",
    "tsup": "8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "8.46.2",
    "vitest": "^4.0.2"
  },
  "volta": {
    "node": "22.20.0",
    "pnpm": "10.19.0"
  }
}
